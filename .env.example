# Supabase (public - needed on client for auth)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# kie.ai (server-only)
KIE_API_KEY=your_kie_api_key

# R2 Storage (server-only)
R2_ACCOUNT_ID=your_cloudflare_account_id
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_BUCKET_NAME=your_r2_bucket_name
R2_PUBLIC_DOMAIN=https://your-r2-public-domain.r2.dev
R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com

# Polar.sh (server-only)
POLAR_ACCESS_TOKEN=your_polar_access_token
POLAR_WEBHOOK_SECRET=your_polar_webhook_secret
POLAR_ORGANIZATION_ID=your_polar_org_id

# Analytics (public - needed on client)
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_project_api_key
